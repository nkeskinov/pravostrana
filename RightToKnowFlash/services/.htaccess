RewriteEngine on
Options FollowSymLinks
RewriteBase /righttoknow/
RewriteRule ^/?bubblechart/([0-9]+)/([0-9]+)$ bubblechart.php?x=$1&y=$2 [L]
RewriteRule ^/?bubblechart/?$ bubblechart.php [L]
RewriteRule ^/?mapchart/([0-9]+)$ mapchart.php?entry_set=$1 [L]
RewriteRule ^/?mapchart/?$ mapchart.php [L]